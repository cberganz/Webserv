[global][global]
root -> $default_root

	[server][$listen_port]
	autoindex -> off
	ip -> $listen_port
	listen -> $listen_port
	port -> $listen_port
	root -> $default_root
	server_name -> $name_server

		[location][$path]
		autoindex -> off
		index -> $default_index
		root -> $default_root

		[location][$path2]
		autoindex -> off
		index -> $index_default
		root -> $default_root

