[global][global]
cgi -> off
root -> $default_root

	[server][$listen_port]
	allowed_methods -> default
	autoindex -> off
	cgi -> off
	client_max_body_size -> 1m
	error_page -> default
	ip -> $listen_port
	listen -> $listen_port
	port -> $listen_port
	root -> $default_root
	server_name -> $name_server

		[location][$path]
		allowed_methods -> default
		autoindex -> off
		cgi -> off
		client_max_body_size -> 1m
		error_page -> default
		index -> $default_index
		ip -> $listen_port
		port -> $listen_port
		root -> $default_root
		server_name -> $name_server

		[location][$path2]
		allowed_methods -> default
		autoindex -> off
		cgi -> off
		client_max_body_size -> 1m
		error_page -> default
		index -> $index_default
		ip -> $listen_port
		port -> $listen_port
		root -> $default_root
		server_name -> $name_server

